# TODO: add default vals?

record(ao, "$(P)$(R)setPulseDiv")
{
	field(DESC, "Pulse Divisor")
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),$(ADDR))PULSE_DIV")
	field(VAL,  3)
    field(DRVL, 0)
    field(DRVH, 13)
    field(PINI, "YES")
    field(PREC, 0)
}
